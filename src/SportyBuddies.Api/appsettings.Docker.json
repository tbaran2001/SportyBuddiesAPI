{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.Seq"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://sportybuddies.seq"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ]
    },
    "ConnectionStrings": {
        "Database": "Host=sportybuddies.db;Database=sportybuddies;Username=postgres;Password=123;",
        "Cache": "sportybuddies.redis:6379"
    },
    "Outbox": {
        "IntervalInSeconds": 10,
        "BatchSize": 10
    }
}
